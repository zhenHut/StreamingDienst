                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="TextBlockStyle"
           TargetType="TextBlock">
        
        <Setter Property="Foreground"
                Value="#FFA9B116" />
        
        <Setter Property="FontSize"
                Value="20" />
        
    </Style>
    
</ResourceDictionary>